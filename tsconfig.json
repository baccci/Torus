{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@models/*": [
        "./src/models/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
    },
  },
  "extends": "astro/tsconfigs/strict"
}